{
  "name": "@adminjs/sql",
  "version": "2.2.2",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "repository": "git@github.com:SoftwareBrothers/adminjs-sql.git",
  "author": "Rafal Dziegielewski <rafal.dzieg@gmail.com>",
  "license": "MIT",
  "keywords": [
    "sql",
    "postgres",
    "adminjs",
    "admin panel",
    "database",
    "adapter"
  ],
  "description": "An official AdminJS adapter for SQL databases.",
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc",
    "lint": "eslint './src/**/*.{ts,js}' --ignore-pattern 'build' --ignore-pattern 'yarn.lock'",
    "release": "semantic-release",
    "test": "npm run test-postgres run && npm run test-mysql run",
    "test-postgres": "DIALECT=postgresql vitest",
    "test-mysql": "DIALECT=mysql vitest"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@semantic-release/git": "^10.0.1",
    "@types/pg": "^8.10.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitest/coverage-istanbul": "^1.4.0",
    "adminjs": "^7.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^4.2.5",
    "rimraf": "^4.4.0",
    "semantic-release": "^20.1.3",
    "semantic-release-slack-bot": "^4.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "knex": "^2.4.2",
    "mysql2": "^3.3.3",
    "pg": "^8.10.0"
  },
  "peerDependencies": {
    "adminjs": "^7.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
